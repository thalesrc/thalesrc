{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "osw",
  "sourceRoot": "apps/osw",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx run osw/shell:serve", "nx run osw/home:serve"],
        "parallel": true
      }
    },
    "mediator": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["docker compose -f apps/osw/docker-compose.mediator.yml up --build -d"]
      }
    }
  }
}
