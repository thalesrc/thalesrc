# FRP Server Configuration Template
# This configuration will be processed by the entrypoint script

# Basic server configuration
bindPort = ${BIND_PORT}

# Dashboard configuration
[webServer]
addr = "0.0.0.0"
port = ${DASHBOARD_PORT}
user = "${DASHBOARD_USER}"
password = "${DASHBOARD_PASSWORD}"

# Authentication method
[auth]
method = "token"
token = "${AUTH_TOKEN}"
