{
  "name": "js-utils",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/js-utils/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "gulp ts:default",
          "gulp ts:commonjs"
        ],
        "cwd": "./libs/js-utils",
        "parallel": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/js-utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
